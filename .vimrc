filetype plugin indent on
syntax on
filetype on
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
set smarttab
set history=1000
color evening
set showmatch
set autoindent
set cindent
set showcmd
set smartindent
highlight Function cterm=bold, ctermbg=red
set filetype=c
set incsearch
set smartcase


